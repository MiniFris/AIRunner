                        <Line X1="0" Y1="105" X2="50" Y2="45" Stroke="Black" StrokeThickness="1"/>
                        <Line X1="0" Y1="105" X2="50" Y2="75" Stroke="Black" StrokeThickness="1"/>
                        <Line X1="0" Y1="105" X2="50" Y2="105" Stroke="Black" StrokeThickness="1"/>
                        <Line X1="0" Y1="105" X2="50" Y2="135" Stroke="Black" StrokeThickness="1"/>
                        <Line X1="0" Y1="105" X2="50" Y2="165" Stroke="Black" StrokeThickness="1"/>
                    </Grid>
                    <Grid>
                        <Line X1="0" Y1="105" X2="50" Y2="15" Stroke="Black" StrokeThickness="1"/>
                        <Line X1="0" Y1="105" X2="50" Y2="45" Stroke="Black" StrokeThickness="1"/>
                        <Line X1="0" Y1="105" X2="50" Y2="75" Stroke="Black" StrokeThickness="1"/>
                        <Line X1="0" Y1="105" X2="50" Y2="105" Stroke="Black" StrokeThickness="1"/>
                        <Line X1="0" Y1="105" X2="50" Y2="135" Stroke="Black" StrokeThickness="1"/>
                        <Line X1="0" Y1="105" X2="50" Y2="165" Stroke="Black" StrokeThickness="1"/>
                    </Grid>
                    <Grid>
                        <Line X1="0" Y1="135" X2="50" Y2="15" Stroke="Black" StrokeThickness="1"/>
                        <Line X1="0" Y1="135" X2="50" Y2="45" Stroke="Black" StrokeThickness="1"/>
                        <Line X1="0" Y1="135" X2="50" Y2="75" Stroke="Black" StrokeThickness="1"/>
                        <Line X1="0" Y1="135" X2="50" Y2="105" Stroke="Black" StrokeThickness="1"/>
                        <Line X1="0" Y1="135" X2="50" Y2="135" Stroke="Black" StrokeThickness="1"/>
                        <Line X1="0" Y1="135" X2="50" Y2="165" Stroke="Black" StrokeThickness="1"/>
                    </Grid>
                </Grid>
                <StackPanel VerticalAlignment="Center">
                    <Ellipse Margin="5" Width="20" Height="20" Fill="#FF00AEFF"/>
                    <Ellipse Margin="5" Width="20" Height="20" Fill="#FF00AEFF"/>
                    <Ellipse Margin="5" Width="20" Height="20" Fill="#FF00AEFF"/>
                    <Ellipse Margin="5" Width="20" Height="20" Fill="#FF00AEFF"/>
                    <Ellipse Margin="5" Width="20" Height="20" Fill="#FF00AEFF"/>
                    <Ellipse Margin="5" Width="20" Height="20" Fill="#FF00AEFF"/>
                </StackPanel>
                <Grid Width="50">
                    <Grid>
                        <Line X1="0" Y1="15" X2="50" Y2="15" Stroke="Black" StrokeThickness="1"/>
                        <Line X1="0" Y1="15" X2="50" Y2="45" Stroke="Black" StrokeThickness="1"/>
                        <Line X1="0" Y1="15" X2="50" Y2="75" Stroke="Black" StrokeThickness="1"/>
                        <Line X1="0" Y1="15" X2="50" Y2="105" Stroke="Black" StrokeThickness="1"/>
                        <Line X1="0" Y1="15" X2="50" Y2="135" Stroke="Black" StrokeThickness="1"/>
                        <Line X1="0" Y1="15" X2="50" Y2="165" Stroke="Black" StrokeThickness="1"/>
                    </Grid>
                    <Grid>
                        <Line X1="0" Y1="45" X2="50" Y2="15" Stroke="Black" StrokeThickness="1"/>
                        <Line X1="0" Y1="45" X2="50" Y2="45" Stroke="Black" StrokeThickness="1"/>
                        <Line X1="0" Y1="45" X2="50" Y2="75" Stroke="Black" StrokeThickness="1"/>
                        <Line X1="0" Y1="45" X2="50" Y2="105" Stroke="Black" StrokeThickness="1"/>

                    </Grid>
                    <Grid Margin="5">
                        <Ellipse Width="20" Height="20" Fill="#FF00AEFF"/>
                        <TextBlock Text="0" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="6" TextAlignment="Center" Foreground="White"/>
                    </Grid>
                    <Grid Margin="5">
                        <Ellipse Width="20" Height="20" Fill="#FF00AEFF"/>
                        <TextBlock Text="0" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="6" TextAlignment="Center" Foreground="White"/>
                    </Grid>
                </StackPanel>
                <Grid Width="50">
